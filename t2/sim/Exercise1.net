* NGSPICE simulation script

* forces current values to be saved
.options savecurrents

.INCLUDE dataNgspice.txt


.control

*makes plots in color
set hcopypscolor=0
set color0=white
set color1=black
set color2=red
set color3=blue
set color4=violet
set color5=rgb:3/8/0
set color6=rgb:4/0/0
set editor=emacs

op

echo "********************************************"
echo  "Operating point"
echo "********************************************"


echo  "Exercise1_TAB"
print @r1[i]
print @r2[i]
print @r3[i]
print @r4[i]
print @r5[i]
print @r6[i]
print @r7[i]
print @gb[i]
print @c1[i]
print @vs[i]
print @hc[i]
print v(1)
print v(2)
print v(3)
print v(5)
print v(6)
print v(7)
print v(8)
echo  "Exercise1_END"

echo  "Data2_DAT"
print v(6)
print v(8)
echo  "Data2_END"

quit
.endc

.end
